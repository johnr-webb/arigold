# Arigold Configuration Example
# Copy this file to .env and fill in your values

# Google Cloud Configuration
ARIGOLD_PROJECT_ID=your-gcp-project-id
ARIGOLD_LOCATION=us-central1
ARIGOLD_REGION=us-central1

# Google API Authentication
# Get your API key from: https://makersuite.google.com/app/apikey
GOOGLE_API_KEY=your-google-api-key

# Agent Configuration
ARIGOLD_AGENT_NAME=Arigold Orchestrator
ARIGOLD_AGENT_DESCRIPTION=An orchestrating agent that coordinates multiple specialized agents

# Model Configuration
ARIGOLD_MODEL_NAME=gemini-2.0-flash-exp
ARIGOLD_TEMPERATURE=0.7
ARIGOLD_MAX_TOKENS=8192

# Logging
ARIGOLD_LOG_LEVEL=INFO

# Cloud Functions Deployment
ARIGOLD_FUNCTION_NAME=arigold-agent
